<!DOCTYPE html>
<html lang="en" metal:use-macro="main_template">
  <body metal:fill-slot="body">
    <script>
      var c = Channel.build({
        origin: '${request.host_url}',
        scope: 'annotator:auth',
        window: window.opener,
        onReady: function () {
          c.call({
            method: 'success',
            params: ${structure: result},
            success: function () { window.close() }
          });
        }
      });
    </script>
  </body>
</html>

